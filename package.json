{
  "name": "command-palette",
  "description": "Awesome Sublime like command palette written in plain javascript",
  "repository": "https://github.com/parro-it/command-palette.git",
  "bugs": {
    "url": "https://github.com/parro-it/command-palette/issues"
  },
  "version": "0.3.0",
  "main": "src/index.js",
  "engines": {
    "node": ">=4.1"
  },
  "keywords": [
    "command",
    "palette",
    "sublime",
    "sublimetext",
    "javascript",
    "css",
    "html"
  ],
  "author": "parro-it",
  "scripts": {
    "cover": "rm -rf coverage && istanbul cover test/index.js && codeclimate-test-reporter < coverage/lcov.info",
    "debug": "hihat --node test -- -p tap-dev-tool",
    "watch": "nodemon test | faucet",
    "test": "quit=1 hihat --node --no-devtool --print --quit --timeout 1000 test | faucet",
    "lint": "eslint src/*.js test/*.js",
    "check": "npm run lint  --silent && npm run test  --silent",
    "release-minor": "npm version minor && npm publish && git push --follow-tags",
    "prepublish": "npm run check"
  },
  "eslintConfig": {
    "extends": "eslint-config-airbnb",
    "plugins": [
      "babel"
    ],
    "rules": {
      "func-names": 0,
      "comma-dangle": 0,
      "no-undef": 1,
      "strict": [
        0,
        "global"
      ]
    }
  },
  "babel": {
    "stage": 4
  },
  "devDependencies": {
    "babel-eslint": "^4.1.3",
    "brfs": "^1.4.1",
    "browserify": "^11.1.0",
    "co": "^4.6.0",
    "codeclimate-test-reporter": "^0.1.1",
    "electron-prebuilt": "^0.33.1",
    "electron-window": "^0.6.0",
    "eslint": "^1.5.0",
    "eslint-config-airbnb": "0.0.8",
    "eslint-plugin-babel": "^2.1.1",
    "eslint-plugin-react": "^3.4.2",
    "faucet": "0.0.1",
    "hihat": "^2.4.3",
    "istanbul": "^0.3.20",
    "nodemon": "^1.7.0",
    "rum-node": "^1.5.0",
    "tap-dev-tool": "^1.3.0",
    "tape-async": "^1.0.2",
    "uglify-js": "^2.4.24"
  },
  "license": "MIT",
  "dependencies": {
    "insert-css": "^0.2.0",
    "jets": "^0.3.0"
  }
}
