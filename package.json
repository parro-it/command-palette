{
  "name": "command-palette",
  "description": "Awesome Sublime like command palette written in plain javascript",
  "repository": "https://github.com/parro-it/command-palette.git",
  "bugs": {
    "url": "https://github.com/parro-it/command-palette/issues"
  },
  "version": "1.0.0",
  "main": "src/index.js",
  "engines": {
    "node": ">=4.1"
  },
  "keywords": [
    "command",
    "palette",
    "sublime",
    "sublimetext",
    "javascript",
    "css",
    "html"
  ],
  "author": "parro-it",
  "scripts": {
    "cover": "cover test/index.js coverage",
    "debug": "collider --dev-tools --run test | faucet",
    "watch": "echo 'not working!'",
    "test": "collider --run test | faucet",
    "check": "npm run lint && npm run test",
    "release-minor": "npm version minor && npm publish && git push --follow-tags",
    "lint": "eslint src test",
    "prepublish": "npm run check"
  },
  "eslintConfig": {
    "extends": "devenv"
  },
  "babel": {
    "stage": 4
  },
  "devDependencies": {
    "brfs": "^1.4.1",
    "browserify": "^11.1.0",
    "esenv": "^1.1.0",
    "rum-node": "^1.12.1",
    "uglify-js": "^2.4.24"
  },
  "license": "MIT",
  "dependencies": {
    "insert-css": "^0.2.0",
    "jets": "^0.3.0"
  }
}
